{
    "version": 2,
    "settings": {
    "color": {
    "palette": [
        { "name": "Primary", "slug": "primary", "color": "#1a1a1a" },
        { "name": "Accent", "slug": "accent", "color": "#ff6f00" },
        { "name": "Background", "slug": "background", "color": "#f5f5f5" }
    ]
    },
    "typography": {
    "fontSizes": [
        { "name": "Small", "slug": "small", "size": "14px" },
        { "name": "Medium", "slug": "medium", "size": "18px" },
        { "name": "Large", "slug": "large", "size": "24px" }
    ]
    }
},
"styles": {
    "blocks": {
    "core/paragraph": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--medium)",
        "color": "var(--wp--preset--color--primary)"
        }
    },
    "core/heading": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--large)",
        "fontWeight": "700",
        "color": "var(--wp--preset--color--accent)"
        }
    },
    "core/list": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--medium)",
        "color": "var(--wp--preset--color--primary)"
        }
    },
    "core/quote": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--large)",
        "fontStyle": "italic",
        "color": "#b22222"
        }
    },
    "core/table": {
        "border": {
        "width": "1px",
        "color": "var(--wp--preset--color--primary)"
        },
        "typography": {
        "fontSize": "var(--wp--preset--font-size--medium)"
        }
    }
    }
}
}