{
    "version": 2,
    "settings": {
    "color": {
    "palette": [
        { "name": "Primary", "slug": "primary", "color": "#1a1a1a" },
        { "name": "Accent", "slug": "accent", "color": "#ff6f00" },
        { "name": "Background", "slug": "background", "color": "#f5f5f5" }
    ]
    },
    "typography": {
    "fontFamilies": [
        { "name": "Poppins", "slug": "poppins", "fontFamily": "'Poppins', sans-serif" }
    ],
    "fontSizes": [
        { "name": "Small", "slug": "small", "size": "14px" },
        { "name": "Medium", "slug": "medium", "size": "18px" },
        { "name": "Large", "slug": "large", "size": "24px" }
    ]
    },
    "layout": {
    "contentSize": "800px",
    "wideSize": "1200px"
    }
},
"styles": {
    "blocks": {
    "core/paragraph": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--medium)",
        "color": "var(--wp--preset--color--primary)"
        }
    },
    "core/heading": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--large)",
        "fontWeight": "700",
        "color": "var(--wp--preset--color--accent)"
        }
    },
    "core/list": {
        "typography": {
        "fontSize": "var(--wp--preset--font-size--medium)",
        "color": "var(--wp--preset--color--primary)"
        }
    }
    }
}
}
